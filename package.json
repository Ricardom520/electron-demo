{
  "name": "askr-yggdrasills",
  "productName": "Askr Yggdrasills",
  "version": "0.0.0",
  "description": "Electron App",
  "license": "",
  "author": "",
  "main": "dist/main.js",
  "scripts": {
    "eslint": "eslint --ext=jsx,ts,tsx ./",
    "eslint:fix": "eslint --fix --ext=jsx,ts,tsx ./",
    "prettier": "prettier --write ./**",
    "dev": "rimraf dist && chcp 65001 && run-p dev:webpack dev:electron",
    "build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --config config/webpack.config.ts --progress",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --config config/webpack.config.ts --progress",
    "dev:electron": "wait-on ./dist/loading.html ./dist/index.html ./dist/main.js && electronmon .",
    "build:packager": "rimraf build && electron-packager . --platform=win32 --arch=x64 --out=./build --electron-version 9.0.2 --app-version 1.0.0 --overwrite --icon=./dist/assets/logo.ico"
  },
  "electronmon": {
    "patterns": [
      "dist/**/*"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "antd": "^5.7.0",
    "axios": "^1.5.0",
    "electron-builder": "23.6.0",
    "electron-packager": "^17.1.1",
    "inversify": "^6.0.1",
    "knex": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "5.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.16.16",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "5.10.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "21.3.5",
    "electronmon": "^2.0.2",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.1",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "mini-css-extract-plugin": "^2.7.6",
    "node-gyp": "8.4.0",
    "node-pre-gyp": "^0.17.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "rimraf": "^5.0.1",
    "schema-utils": "^4.2.0",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.3",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "wait-on": "^7.0.1",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "none",
    "printWidth": 100
  }
}
